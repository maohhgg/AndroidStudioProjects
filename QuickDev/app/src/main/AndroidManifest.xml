<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mao.quickdev">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".itemindex.ItemIndexActivity"
            android:label="ItemIndex" />
        <activity
            android:name=".progressbar.ProgressActivity"
            android:label="ProgressBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".viewpage.ViewPageActivity"
            android:theme="@style/FullScreen" />
        <activity
            android:name=".viewpageranmi.AnimaActivity"
            android:theme="@style/FullScreen"/>
        <activity
            android:name=".viewpageranmi.ZoomOutActivity"
            android:theme="@style/FullScreen" />
        <activity
            android:name=".viewpageranmi.DepthActivity"
            android:theme="@style/FullScreen" />
    </application>

</manifest>